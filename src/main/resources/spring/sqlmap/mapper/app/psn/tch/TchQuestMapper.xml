<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="app.psn.tch.quest.mapper.TchQuestMapper">

    <update id="questProc" parameterType="app.psn.tch.quest.vo.ReqQuestProcVO">
        UPDATE QUEST_LOGS
        SET STATUS       = #{status}
          , PROCESSED_BY = #{processedBy}
        WHERE LOG_SN = #{logSn};
    </update>

</mapper>